{
    "name": "dennisvandalen/xfdf-php",
    "description": "This is my package xfdf-php",
    "keywords": [
        "dennisvandalen",
        "xfdf-php"
    ],
    "homepage": "https://github.com/dennisvandalen/xfdf-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis van Dalen",
            "email": "hi@vandalen.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "DennisVanDalen\\XfdfPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DennisVanDalen\\XfdfPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
